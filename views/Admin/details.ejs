<body>

<!-- Navbar -->
<nav class="navbar-top fixed w-full z-20 top-0 left-0">
    <div class="max-w-screen-xl flex flex-wrap items-center justify-between mx-auto p-4">
        <a href="#" class="flex items-center">
            <img src="https://flowbite.com/docs/images/logo.svg" class="h-8 mr-3" alt="Logo">
            <span class="self-center text-2xl font-semibold whitespace-nowrap dark:text-white">CareSphere</span>
        </a>
        <div class="profile"></div>
        <button class="btun" data-bs-toggle="modal" data-bs-target="#mainModal">
            <span class="icon">
                <svg viewBox="0 0 175 80" width="40" height="40">
                    <rect width="80" height="15" fill="#f0f0f0" rx="10"></rect>
                    <rect y="30" width="80" height="15" fill="#f0f0f0" rx="10"></rect>
                    <rect y="60" width="80" height="15" fill="#f0f0f0" rx="10"></rect>
                </svg>
            </span>
            <span class="text">MENU</span>
        </button>
    </div>
</nav>

<!-- Main Modal -->
<div class="main-modal">
    <div class="modal fade" id="mainModal" data-bs-overflow="hidden" tabindex="-1" aria-labelledby="mainModalLabel" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <% if (data.Lname == '' && data.Mname == ''){ %>
                        <div class="profimg"><img src="<%= data.image %>" alt=""></div> <h1 class="modal-title fs-5" id="mainModalLabel"><%= data.Fname %></h1>
                        <% }else if(data.Mname == ''){ %>
                        <div class="profimg"><img src="<%= data.image %>" alt=""></div> <h1 class="modal-title fs-5" id="mainModalLabel"><%= data.Fname %> <%= data.Lname %></h1>
                        <% }else{ %>
                        <div class="profimg"><img src="<%= data.image %>" alt=""></div> <h1 class="modal-title fs-5" id="mainModalLabel"><%= data.Fname %> <%= data.Mname %> <%= data.Lname %></h1>
                        <% } %>
                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close">
                        <span class="material-symbols-outlined">
                            close
                        </span>
                    </button>
                </div>
                <div class="modal-body">
                    <div class="list">
                        <ul>
                            <a href="/admin?username=<%= data.username %>"><li><span>Home</span></li></a>
                            <a href="/doctorinfo?username=<%= data.username %>"><li><span>Doctors</span></li></a>
                            <a href="/staffinfo?username=<%= data.username %>"><li><span>Staff</span></li></a>
                            <a href="/patientinfo?username=<%= data.username %>"><li><span>Patients</span></li></a>
                            <a href="/changelog?username=<%= data.username %>"><li><span>ChangeLogs</span></li></a>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

