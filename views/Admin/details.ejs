                <div class="col-lg-2"><%= val[i].room %></div>
                <div class="col-lg-2">
                    <button type="button" class="self-center mx-auto bten btun qual" data-bs-toggle="modal" data-bs-target="#depModal<%= val[i].id %>" style="padding-left: 3rem;">View</button>
