                    <div class="col-lg-2 docname"><%= val[i].Fname %> <%= val[i].Lname %></div>
                <% }else{ %>
                    <div class="col-lg-2 docname"><%= val[i].Fname %> <%= val[i].Mname %> <%= val[i].Lname %></div>
