                    </form>
                </div>
            </div>
            <% } %>
        </div>
    <% } %>

</div>
<% for(let i= 0; i < val.length; i++ ){ %> 
    <% if(type == "Doctor"){ %>
        <!-- Qualification Modal for each doctor -->
        <div class="qual-modal">
            <div class="modal fade" id="qualModal<%= val[i].id %>" tabindex="-1" aria-labelledby="qualModalLabel<%= val[i].id %>" aria-hidden="true" data-backdrop="false" style="overflow: hidden;">
                <div class="modal-dialog">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h5 class="modal-title" id="qualModalLabel<%= val[i].id %>">Qualification for <%= val[i].Fname %> <%= val[i].Mname %> <%= val[i].Lname %></h5>
                            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                        </div>
                        <div class="modal-body">
                            <% for(let j = 0; j < qualval.length; j++){ %>
                                <% if(qualval[j].doctorId == val[i].id){ %>
                                    <div class="row">
                                        <div class="col-lg-4"><h3 class="col">Degree : </h1> <p><%= qualval[j].degree %></p></div>
                                        <div class="col-lg-4"><h3 class="col">Year : </h3><p><%= qualval[j].year %></p></div>
                                        <div class="col-lg-4"><h3 class="col">Institute : </h3><p><%= qualval[j].institute %></p></div>
                                    </div>
                                    <div class="row">
                                        <img src="<%= qualval[j].degree_img %>" style="height: 17rem;"></img>
                                    </div>
                                <% } %>
                            <% } %>
                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    <% }else if(type == "Staff"){ %>
        <div class="qual-modal">
            <div class="modal fade" id="qualModal<%= val[i].id %>" tabindex="-1" aria-labelledby="qualModalLabel<%= val[i].id %>" aria-hidden="true" data-backdrop="false" style="overflow: hidden;">
                <div class="modal-dialog">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h5 class="modal-title" id="qualModalLabel<%= val[i].id %>">Qualification for <%= val[i].Fname %> <%= val[i].Mname %> <%= val[i].Lname %></h5>
                            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                        </div>
                        <div class="modal-body">
                            <% for(let j = 0; j < qualval.length; j++){ %>
                                <% if(qualval[j].staffId == val[i].id){ %>
                                    <div class="row">
                                        <div class="col-lg-4"><h3 class="col">Degree : </h1> <p><%= qualval[j].degree %></p></div>
                                        <div class="col-lg-4"><h3 class="col">Year : </h3><p><%= qualval[j].year %></p></div>
                                        <div class="col-lg-4"><h3 class="col">Institute : </h3><p><%= qualval[j].institute %></p></div>
                                    </div>
                                    <div class="row">
                                        <img src="<%= qualval[j].degree_img %>" style="height: 17rem;"></img>
                                    </div>
                                <% } %>
                            <% } %>
                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    <% }else{ %>
        <div class="qual-modal">
            <div class="modal fade" id="depModal<%= val[i].id %>" tabindex="-1" aria-labelledby="depModalLabel<%= val[i].id %>" aria-hidden="true" data-backdrop="false">
                <div class="modal-dialog">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h5 class="modal-title" id="depModalLabel<%= val[i].id %>">Dependants of <%= val[i].Fname %> <%= val[i].Mname %> <%= val[i].Lname %></h5>
                            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                        </div>
                        <div class="modal-body">
                            <% for(let j = 0; j < depval.length; j++){ %>
                                <% if(depval[j].patientid == val[i].id){ %>
                                    <div class="col-lg-4"><h3 class="col" style="width: 25rem;">Relation : <%= depval[j].relation %></h3></div>
                                    <div class="col-lg-4"><h3 class="col" style="width: 25rem;">Name : <%= depval[j].Fname %> <%= depval[j].Mname %> <%= depval[j].Lname %></h1></div>
                                    <div class="col-lg-4"><h3 class="col" style="width: 25rem;">Phone :  <%= depval[j].phone %></h3></div>
                                    <div class="col-lg-4"><h3 class="col" style="width: 25rem;">Email : <%= depval[j].email %></h3></div>
                                    <div class="col-lg-4"><h3 class="col" style="width: 25rem;">Sex : <%= depval[j].sex %></h3></div>
                                    <div class="col-lg-4"><h3 class="col" style="width: 25rem;">Date of Birth : <%= depval[j].dob %></h3></div>
                                    <div class="col-lg-4"><h3 class="col" style="width: 25rem; margin-bottom: 5rem;">Age : <%= depval[j].age %></h3></div>
                                <% } %>
                            <% } %>
                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    <% } %>
<% } %>
<script src="JS/details.js"></script>
<script src="https://cdn.jsdelivr.net/npm/toastify-js"></script>

<% if (createStatus.success === true) { %>
    <script>
        document.addEventListener('DOMContentLoaded', function() {
            showToast("<%= createStatus.message %>", "<%= createStatus.type %>");
        });

        function showToast(message, type) {
            Toastify({
                text: message,
                duration: 3000,
                newWindow: true,
                close: true,
                gravity: "bottom", // top or bottom
                position: "right",
                offset: {
                    x: 16,
                    y: -16
                },
                backgroundColor: "black",
                className: "custom-toast-style",
                style: {
                    padding: "3rem", // Add your desired padding
                }
            }).showToast();
        }
    </script>
<% } %>

</body>
</html>