<div class="containers">
    <h4><%= type %>'s List</h4>
    <div class="row srow">
        <div class="col-lg-6 search">
            <div class="search-box">
                <button class="btn-search"><i class="fas fa-search"></i></button>
                <input id="searchbar" type="text" class="input-search" name="search" placeholder="Type to Search...">
            </div>
        </div>
        <div class="col-lg-6 new-but">
            <button class="button reg-modal" data-bs-toggle="modal" data-bs-target="#newDoctorModal"> 
                + New
            </button>
        </div>
    </div>


    <% if(type == "Doctor"){ %>
        <div class="docinfo">
            <div class="row head">
                <div class="col-lg-2">Name</div>
                <div class="col-lg-2">Designation</div>
                <div class="col-lg-2">Specialty</div>
                <div class="col-lg-2">Field</div>
                <div class="col-lg-2">Qualification</div>
                <div class="col-lg-2"></div>
            </div>
            <% for(let i= 0; i < val.length; i++ ){ %>
            <div class="row tuple">
                <% if (val[i].Lname == '' && val[i].Mname == ''){ %>
                    <div class="col-lg-2 docname"><%= val[i].Fname %></div>
                <% }else if(val[i].Mname == ''){ %>
                    <div class="col-lg-2 docname"><%= val[i].Fname %> <%= val[i].Lname %></div>
                <% }else{ %>
                    <div class="col-lg-2 docname"><%= val[i].Fname %> <%= val[i].Mname %> <%= val[i].Lname %></div>
                <% } %>
                <div class="col-lg-2"><%= val[i].designation %></div>
                <div class="col-lg-2"><%= val[i].speciality %></div>
                <div class="col-lg-2"><%= val[i].field %></div>
                <div class="col-lg-2">
                    <button type="button" class="self-center mx-auto bten btun qual" data-bs-toggle="modal" data-bs-target="#qualModal<%= val[i].id %>" style="padding-left: 3rem;">View</button>
                </div>
                <div class="col-lg-2">
                    <form action="/singledocinfo" method="post">
                        <input type="hidden" name="id" value="<%= val[i].id %>">
                        <input type="hidden" name="adminid" value="<%= data.id %>">
                        <button class="bten btun" type="submit">Details</button>
                    </form>
                </div>
            </div>
            <% } %>
        </div>
    <% }else if(type == "Staff"){ %>
        <div class="docinfo">
            <div class="row head">
                <div class="col-lg-2">Name</div>
                <div class="col-lg-2">Designation</div>
                <div class="col-lg-2">Experience</div>
                <div class="col-lg-2">Room</div>
                <div class="col-lg-2">Qualification</div>
                <div class="col-lg-2"></div>
            </div>
            <% for(let i= 0; i < val.length; i++ ){ %>
            <div class="row tuple">
                <% if (val[i].Lname == '' && val[i].Mname == ''){ %>
                    <div class="col-lg-2 docname"><%= val[i].Fname %></div>
                <% }else if(val[i].Mname == ''){ %>
                    <div class="col-lg-2 docname"><%= val[i].Fname %> <%= val[i].Lname %></div>
                <% }else{ %>
                    <div class="col-lg-2 docname"><%= val[i].Fname %> <%= val[i].Mname %> <%= val[i].Lname %></div>
                <% } %>
                <div class="col-lg-2"><%= val[i].designation %></div>
                <div class="col-lg-2"><%= val[i].experience %></div>
                <div class="col-lg-2"><%= val[i].room %></div>
                <div class="col-lg-2">
                    <button type="button" class="self-center mx-auto bten btun qual" data-bs-toggle="modal" data-bs-target="#qualModal<%= val[i].id %>" style="padding-left: 3rem;">View</button>
                </div>
                <div class="col-lg-2">
                    <form action="/singlestaffinfo" method="post">
                        <input type="hidden" name="id" value="<%= val[i].id %>">
                        <input type="hidden" name="adminid" value="<%= data.id %>">
                        <button class="bten btun" type="submit">Details</button>
                    </form>
                </div>
            </div>
            <% } %>
        </div>
    <% }else{ %>
        <div class="docinfo">
            <div class="row head">
                <div class="col-lg-2">Name</div>
                <div class="col-lg-2">Phone</div>
                <div class="col-lg-2">Email</div>
                <div class="col-lg-2">Room</div>
                <div class="col-lg-2">Dependants</div>
                <div class="col-lg-2"></div>
            </div>
            <% for(let i= 0; i < val.length; i++ ){ %>
            <div class="row tuple">
                <% if (val[i].Lname == '' && val[i].Mname == ''){ %>
                    <div class="col-lg-2 docname"><%= val[i].Fname %></div>
